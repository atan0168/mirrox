{
  "name": "digital-twin-backend",
  "version": "1.0.0",
  "description": "Backend service for Digital Twin Air Quality application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json}\" \"*.{js,ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,json}\" \"*.{js,ts,json}\"",
    "seed:nutrition": "ts-node scripts/seed_nutrition.ts"
  },
  "keywords": [
    "air-quality",
    "api",
    "express",
    "typescript"
  ],
  "author": "Digital Twin Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "better-sqlite3": "^11.10.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.17",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.10.0",
    "file-type": "^16.5.4",
    "form-data": "^4.0.4",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1",
    "zod": "^4.1.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.8.3"
  }
}
