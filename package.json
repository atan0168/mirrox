{
  "name": "digital-twin-workspace",
  "version": "1.0.0",
  "description": "Digital Twin Air Quality Application Workspace",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:app": "cd app && npm run format",
    "format:backend": "cd backend && npm run format",
    "lint": "npm run lint:app && npm run lint:backend",
    "lint:app": "cd app && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "install:all": "npm install && cd app && npm install && cd ../backend && npm install",
    "prepare": "husky",
    "build:backend": "cd backend && npm run build",
    "build": "npm run build:backend",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "expo": "^51.0.39",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "zustand": "^5.0.8"
  }
}
