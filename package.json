{
  "name": "digital-twin-workspace",
  "version": "1.0.0",
  "description": "Digital Twin Air Quality Application Workspace",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:app": "cd app && npm run format",
    "format:backend": "cd backend && npm run format",
    "lint": "npm run lint:app && npm run lint:backend",
    "lint:app": "cd app && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "install:all": "npm install && cd app && npm install && cd ../backend && npm install",
    "prepare": "husky",
    "build:backend": "cd backend && npm run build",
    "build": "npm run build:backend",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.90.2",
    "dayjs": "^1.11.18",
    "expo": "^51.0.39",
    "expo-application": "~5.9.1",
    "expo-asset": "~10.0.10",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-gl": "~14.0.2",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-location": "~17.0.1",
    "expo-media-library": "~16.0.5",
    "expo-notifications": "~0.28.19",
    "expo-random": "~14.0.1",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.1",
    "expo-task-manager": "~11.8.2",
    "expo-updates": "~0.25.28",
    "lucide-react-native": "^0.545.0",
    "react": "18.2.0",
    "react-hook-form": "^7.65.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "zustand": "^5.0.8"
  }
}
